&GLOBAL
  PRINT_LEVEL LOW
  PROJECT h2_gpw_ht_group2
  RUN_TYPE ENERGY
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    &ACTIVE_SPACE
      ACTIVE_ELECTRONS 2
      ACTIVE_ORBITALS 2
      REMOVE_REF
      &ERI
        METHOD GPW_HALF_TRANSFORM
        PERIODICITY 1 1 1
      &END ERI
      &ERI_GPW
        CUTOFF 200
        GROUP_SIZE 1
      &END ERI_GPW
      &FCIDUMP
        FILENAME __STD_OUT__
      &END FCIDUMP
    &END ACTIVE_SPACE
    &MGRID
      CUTOFF 300
    &END MGRID
    &POISSON
      PERIODIC XYZ
      POISSON_SOLVER ANALYTIC
    &END POISSON
    &QS
      METHOD GPW
    &END QS
    &SCF
      ADDED_MOS 3
      MAX_SCF 100
      &PRINT
        &RESTART OFF
        &END RESTART
      &END PRINT
    &END SCF
    &XC
      &HF 1.0
        &INTERACTION_POTENTIAL
          CUTOFF_RADIUS 1.9
          POTENTIAL_TYPE TRUNCATED
        &END INTERACTION_POTENTIAL
      &END HF
      &XC_FUNCTIONAL NONE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
      PERIODIC XYZ
    &END CELL
    &COORD
      H 0.000 0.000  0.356
      H 0.000 0.000 -0.356
    &END COORD
    &KIND H
      BASIS_SET DZV-GTH
      POTENTIAL GTH-HF
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
